<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
	<title>emil @ day</title>
	<description>the blog you should(nt?) read, boi</description>
	<link>https://liech.ga/d</link>
	<atom:link href="https://liech.ga/d/micro.xml" rel="self" type="application/rss+xml" />
	{ % for post in site.micro % }
	  <item>
		<title>{{ post.title}}</title>
		<description>
		  {{ post.content}}</description>
		</description>
		<pubDate>{{ post.date | date_to_rfc822 }}</pubDate>
		<link>{{ post.url | prepend: site.baseurl | prepend: site.url }}/index.html</link>
		<guid isPermaLink="true">{{ post.url | prepend: site.baseurl | prepend: site.url }}</guid>
	  </item>
	{ % endfor % }
  </channel>
</rss>
